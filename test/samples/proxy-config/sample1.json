{
  "requestOptions": {
    "getAllPets": {
      "uri": "\"https://petfinder.com/pets\"",
      "method": "GET",
      "messagePropertyMap": {
        "bandit": "Bandit",
        "banditCaptain": "bandit_captain"
      },
      "body": ["Bandit"],
      "qs": ["bandit_captain"]
    },
    "createPet": {
      "uri": "\"https://petfinder2.com/pets\"",
      "method": "POST",
      "body": {
        "pet_type": "options.petType",
        "pet_name": "options.petName",
        "pet_attitude": "options.petAttitude"
      }
    },
    "getPet": {
      "uri": "\"https://petfinder2.com/pets/#{options.petId}\"",
      "method": "GET"
    }
  }
}
